ul.nav.justify-content-center.nav-pills.nav-news(data-select)
	each val, index in ['Kiến thức làm đẹp', 'Sự kiện làm đẹp', 'Tuyển dụng']
		if(index == 0)
			li.nav-item.text-center.active
				a.nav-link.text-white.p-0(href="javascript:void(0)")= val
		else
			li.nav-item.text-center
				a.nav-link.p-0(href="javascript:void(0)")= val
