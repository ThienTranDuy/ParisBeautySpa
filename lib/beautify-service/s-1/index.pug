section.canhcam-beautify-service-news-3
	article.news_list
		.container
			//- Title
			.row
				.col-10.mx-auto
					h1.h1.text-center.text-uppercase Tin tức
			//- Navtab
			.row
				.col
					include ../../nav-tab/n-1/index.pug
			//- Content here
